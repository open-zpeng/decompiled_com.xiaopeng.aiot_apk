    <item android:state_enabled="false">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/fragrance_channel_background_radius"/>
            <size android:height="66dp"/>
            <solid android:color="#29ffffff"/>
        </shape>
    </item>
</selector>
